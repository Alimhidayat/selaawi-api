{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/__init__.py", 
        "methods": ["GET"]
      },
      {
        "src": "/dashboard/dataApi1",
        "dest": "app/blueprints/dashboard/bp_dashboard.py",
        "methods": ["GET"]
      },
      {
        "src": "/real-time/dataApi2\\?start_date=(.*)&end_date=(.*)",
        "dest": "app/blueprints/real_time/real_time.py?start_date=$1&end_date=$2",
        "methods": ["GET"]
      }
    ]
  }